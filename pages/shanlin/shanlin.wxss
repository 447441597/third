/* pages/tianfu/tianfu.wxss */
.datu image {
  width: 680rpx;
  height: 417rpx;
}
.datu {
  width: 750rpx;
  text-align: center;
}
.item {
  width: 330rpx;
  height: 250rpx;
  margin-bottom: 60rpx;
}
.item image {
  width: 100%;
  height: 200rpx;
}
.money {
  padding: 0 35rpx;
  margin-top: 35rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.money .item .card {
  font-size: 30rpx;
  display: block;
}
.money .item .price {
  font-size: 30rpx;
  display: block;
}
.money .item .aside {
  font-size: 22rpx;
  color: #d8d8d8;
}
.money .item .item-text {
  display: flex;
  justify-content: space-between;
 padding: 0 30rpx;
}
.header{
   display: block;
   padding: 0 35rpx;
   height: 300rpx;
   display: flex;
   justify-content: space-between;
   align-items: center
}
.header image{
    width: 100rpx;
    height: 100rpx;
    margin-bottom: 50rpx
}
.header .head text{
    display: block;
    font-size: 28rpx;
    color: #acacac
}
.header .head{
    text-align: center
}
