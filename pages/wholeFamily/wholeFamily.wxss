/* pages/wholeFamily/wholeFamily.wxss */
.header{
    width: 750rpx;
    height: 250rpx;
    display: flex;
    justify-content: space-around;
    /* padding-left: rpx; */
    padding-right: 50rpx
}

.header .one image{
    width: 120rpx;
    height: 120rpx;
}
 
.header .one text{
    color: #7c7c7c;
}
.header image{
    width: 70rpx;
    height: 70rpx;
    display: block;
}
.header text{
    display: block;
    color: #acacac;
    font-size: 22rpx;
    text-align: center;
    margin-top: 50rpx;
}

.banner{
    width: 750rpx;
    text-align: center;
}
.banner image{
    width: 700rpx;
    height: 340rpx;
}

.main{
    width: 750rpx;
}
.top{
    width: 750rpx;
    display: flex;
    align-items: center;
    margin-top: 40rpx;
}
.top image{
    width: 60rpx;
    height: 40rpx;
    margin-left: 30rpx;
}
.top text{
    font-size: 30rpx;
    color: #7c7c7c;
    margin-left: 14rpx;
}
.moneyAll{
    width: 750rpx;
    padding-left: 30rpx;
    padding-right: 30rpx;
    display: flex;
    justify-content: space-between;
    margin-top: 20rpx;
}
.moneyAll .money{
    width: 690rpx;
}
.moneyAll .money image{
    width: 320rpx;
    height: 200rpx;
}
.money text{
    font-size: 24rpx;
    color: #7c7c7c;
}

.moneyAll .money .textTop{
    width: 320rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 5rpx 10rpx;
}

.moneyAll .money .textTop .five{
    font-size: 28rpx;
}

.moneyAll .money .textTop .redMoney{
    color: #d93e3a
}

.moneyAll .money .textTop .noMoney{
    font-size: 20rpx;
    margin-right: 19rpx;
    margin-left: 5rpx;
    text-decoration:line-through; 
}

.moneyAll .money .textBotton{
    margin-left: 30rpx;
}
.none{
    visibility: hidden;
}